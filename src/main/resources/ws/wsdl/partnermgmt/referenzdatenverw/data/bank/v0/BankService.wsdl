<?xml version="1.0" encoding="utf-8"?>
<wsdl:definitions name="BankService"
                  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                  targetNamespace="urn:com:adcubum:syrius:api:partnermgmt:referenzdatenverw:data:bank:v0"
                  xmlns:tns="urn:com:adcubum:syrius:api:partnermgmt:referenzdatenverw:data:bank:v0"
                  xmlns:pa="urn:com:adcubum:syrius:api:partnermgmt:referenzdatenverw:data:bank:v0:schema"
                  xmlns:flt="urn:com:adcubum:syrius:api:common:faults:v_1_0"
                  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
                  xmlns:http="http://schemas.xmlsoap.org/wsdl/http/">
    <wsdl:types>
        <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="urn:com:adcubum:syrius:api:partnermgmt:referenzdatenverw:data:bank:v0:schema" schemaLocation="schema/BankService.xsd"/>
            <xsd:import namespace="urn:com:adcubum:syrius:api:common:faults:v_1_0" schemaLocation="../../../../../common/faults/v_1_0/faults.xsd"/>
        </xsd:schema>
    </wsdl:types>
    <wsdl:message name="GetBankRequestMessage">
        <wsdl:part name="request" element="pa:getBankRequest"/>
    </wsdl:message>
    <wsdl:message name="GetBankResponseMessage">
        <wsdl:part name="response" element="pa:getBankResponse"/>
    </wsdl:message>
    <wsdl:message name="ApiFault">
        <wsdl:part name="fault" element="flt:apiFaultInfo"/>
    </wsdl:message>
    <wsdl:portType name="BankServicePortType">
        <wsdl:operation name="getBank">
            <wsdl:documentation>Liefert für einen technischen Bankschlüssel die Daten der zugehörigen Bank.</wsdl:documentation>
            <wsdl:input name="GetBankRequest" message="tns:GetBankRequestMessage"/>
            <wsdl:output name="GetBankResponse" message="tns:GetBankResponseMessage"/>
            <wsdl:fault name="ApiFault" message="tns:ApiFault"/>
        </wsdl:operation>
    </wsdl:portType>
    <wsdl:binding name="BankServiceBinding" type="tns:BankServicePortType">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <wsdl:operation name="getBank">
            <soap:operation soapAction="getBank" style="document"/>
            <wsdl:input name="GetBankRequest">
                <soap:body use="literal"/>
            </wsdl:input>
            <wsdl:output name="GetBankResponse">
                <soap:body use="literal"/>
            </wsdl:output>
            <wsdl:fault name="ApiFault">
                <soap:fault name="ApiFault" use="literal"/>
            </wsdl:fault>
        </wsdl:operation>
    </wsdl:binding>
    <wsdl:service name="BankService_v0">
        <wsdl:port name="BankService" binding="tns:BankServiceBinding">
            <soap:address location="https://example.com/BankService_v0"/>
        </wsdl:port>
    </wsdl:service>
</wsdl:definitions>
