        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="partnerEmail" type="tns:WsPartnerEmailType" minOccurs="1" maxOccurs="unbounded">
               <xsd:annotation>
                    <xsd:documentation xml:lang="de">Enthält die geladenen E-Mail-Adressen eines Partners.</xsd:documentation>
                </xsd:annotation>
